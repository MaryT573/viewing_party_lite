<div>
  <table>
    <thead>
      <tr>
        <th>Users</th>
      </tr>
    </thead>
    <tbody>
    <% if current_user %>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.email %></td>
        </tr>
      <% end %>
    <% end %>
    </tbody>
  </table>
</div>

<%= link_to "Create User", new_user_path %>
<% if !current_user %>
  <%= link_to "Log In", "/login" %>
<% end %>

